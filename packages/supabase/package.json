{
    "name": "@onlook/supabase",
    "description": "Supabase client library for Onlook",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "clean": "rm -rf node_modules",
        "lint": "eslint --fix .",
        "format": "prettier --write .",
        "db:gen": "supabase gen types --lang=typescript --project-id $PROJECT_ID --schema public > src/types/db.ts",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "onlook",
        "supabase"
    ],
    "author": {
        "name": "Onlook",
        "email": "contact@onlook.dev"
    },
    "license": "Apache-2.0",
    "homepage": "https://onlook.dev",
    "dependencies": {
        "@supabase/supabase-js": "^2.45.6",
        "react": "^18.3.1"
    },
    "devDependencies": {
        "@onlook/typescript": "*",
        "supabase": "^1.207.9",
        "typescript": "^5.5.4"
    },
    "exports": {
        "./clients": "./src/clients/index.ts",
        "./queries": "./src/queries/index.ts",
        "./rpc": "./src/rpc/index.ts",
        "./types": "./src/types/index.ts"
    }
}
