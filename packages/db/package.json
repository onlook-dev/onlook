{
    "name": "@onlook/db",
    "description": "Drizzle database schema",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "scripts": {
        "lint": "eslint . --max-warnings 0",
        "format": "eslint --fix .",
        "typecheck": "tsc --noEmit",
        "db:gen": "drizzle-kit generate",
        "db:push": "drizzle-kit push",
        "db:studio": "drizzle-kit studio",
        "db:migrate": "drizzle-kit migrate",
        "db:seed": "bun src/seed/seed.ts",
        "db:seed:stripe": "bun src/seed/stripe/stripe.ts",
        "db:reset": "bun src/seed/reset.ts",
        "db:drop": "drizzle-kit drop",
        "db:migrate:branching": "bun src/migration-scripts/migrate-to-branching.ts"
    },
    "dependencies": {
        "dotenv": "^17.2.1",
        "drizzle-orm": "^0.44.5",
        "drizzle-zod": "^0.8.3",
        "pg": "^8.16.3",
        "postgres": "^3.4.7",
        "uuid": "^11.1.0",
        "@onlook/stripe": "*"
    },
    "devDependencies": {
        "@onlook/eslint": "*",
        "drizzle-kit": "^0.31.4",
        "typescript": "^5.8.2",
        "ai": "5.0.26"
    }
}