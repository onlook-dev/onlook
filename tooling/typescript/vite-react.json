{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Vite React",
    "extends": "./base.json",
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020", "DOM", "DOM.Iterable"],
      "useDefineForClassFields": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react-jsx"
    }
  }